<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context=".Komprehensyon">




    <TextView
        android:id="@+id/textView4"
        android:layout_width="231dp"
        android:layout_height="83dp"
        android:text="KOMPREHENSYON"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.166"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.074"
        tools:ignore="MissingConstraints"></TextView>


    <Button
        android:id="@+id/button"
        android:layout_width="350dp"
        android:layout_height="66dp"
        android:backgroundTint="#EE4211"
        android:text="ANG ALAMAT NG ROSAS"
        android:textColor="@android:color/white"
        android:textSize="17dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.215"
        tools:ignore="MissingConstraints" />


    <Button
        android:id="@+id/button2"
        android:layout_width="350dp"
        android:layout_height="66dp"
        android:backgroundTint="#E0B782"
        android:text="ANG KWENTO NI LINO AT ANG KANIYANG LOBONG PANYO"
        android:textAlignment="textStart"
        android:textColor="@android:color/white"
        android:textSize="17dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.327"
        tools:ignore="MissingConstraints" />


    <Button
        android:id="@+id/button3"
        android:layout_width="350dp"
        android:layout_height="66dp"
        android:backgroundTint="#E0B782"
        android:text="ANG ALAMAT NG PUNONG MANGGA"
        android:textAlignment="viewStart"
        android:textColor="@android:color/white"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.442"
        tools:ignore="MissingConstraints" />


    <FrameLayout
        android:id="@+id/frameLayout3"
        android:layout_width="38dp"
        android:layout_height="36dp"
        android:background="@drawable/circle_bg"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="327dp"
        tools:layout_editor_absoluteY="231dp">


        <ImageView
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_gravity="center"
            android:src="@drawable/ic_lock" />
    </FrameLayout>


    <FrameLayout
        android:layout_width="38dp"
        android:layout_height="36dp"
        android:background="@drawable/circle_bg"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="327dp"
        tools:layout_editor_absoluteY="306dp">


        <ImageView
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_gravity="center"
            android:src="@drawable/ic_lock" />
    </FrameLayout>


</androidx.constraintlayout.widget.ConstraintLayout>

